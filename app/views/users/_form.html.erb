<%= form_for user do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age  %>
  <%= f.number_field :age  %>
  <%= f.label :favorite_books  %>
  <%= f.text_field :favorite_books %>
  <%= f.label :password  %>
  <%= f.password_field :password  %>  
  <%= f.label :password_confirmation  %>
  <%= f.password_field :password_confirmation  %>
  <%= f.submit  %>
<% end %>